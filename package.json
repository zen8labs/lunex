{
  "name": "llm-client",
  "private": true,
  "version": "0.1.0-alpha.28",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "gen:bindings": "cd src-tauri && cargo test --package nexo --lib constants::tests::generate_typescript_bindings -- --nocapture",
    "tauri:dev": "yarn gen:bindings && tauri dev",
    "tauri:build": "yarn gen:bindings && tauri build",
    "i18n:scan": "i18next-scanner --config i18next-scanner.config.cjs",
    "i18n:extract": "i18next-scanner --config i18next-scanner.config.cjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint:rust": "cd src-tauri && cargo clippy -- -D warnings",
    "format:rust": "cd src-tauri && cargo fmt",
    "format:rust:check": "cd src-tauri && cargo fmt -- --check",
    "docs:dev": "cd docs && yarn start",
    "docs:build": "cd docs && yarn build",
    "prepare": "husky"
  },
